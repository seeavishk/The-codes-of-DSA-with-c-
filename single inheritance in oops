#include<iostream>
using namespace std;
class animal{
    public:
    int age ;
    int weight;
   public:
   void speak(){
    cout<<"speaking"<<endl;
   }
};
class dog:public animal{

};//multiple inheritance
class labra:public dog{

};
int main()
{
    dog d;
    d.speak();
    cout<<d.age<<endl;
    labra l;
    l.speak();
}
